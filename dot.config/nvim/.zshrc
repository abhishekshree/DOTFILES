# Some random bad assignments but ok
alias q='clear'
alias ee='exit'
alias aa='cd ~/Desktop/codeforces/contests/contest'
alias u='sudo apt-get update && sudo apt-get -y upgrade'
alias ra='rm -rf'

mdd() {
    mkdir -p -- "$1" &&
      cd -P -- "$1"
}

# cf aliases
alias cf='/home/abhishek/cf'
alias test='cf test'
alias parse='cf parse '
alias submit='cf submit'

alias open='xdg-open .'
alias scroll-lock="xmodmap -e 'add mod3 = Scroll_Lock'"
alias lights='xset led on'
alias lights-off='xset led off'
# Python venv
alias ve='python -m venv ./venv'
alias va='source ./venv/bin/activate'

# Go setup
export GOROOT=/usr/local/go
export PATH=$PATH:$GOROOT/bin
export GOPATH=/home/abhishek/golib
export PATH=$PATH:$GOPATH/bin
export GOPATH=$GOPATH:/home/abhishek/code

export PATH="$HOME/.yarn/bin:$HOME/.config/yarn/global/node_modules/.bin:$PATH"
source ~/powerlevel10k/powerlevel10k.zsh-theme

# To customize prompt, run `p10k configure` or edit ~/.p10k.zsh.
[[ ! -f ~/.p10k.zsh ]] || source ~/.p10k.zsh
